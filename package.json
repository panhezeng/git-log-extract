{
  "name": "git-log-extract",
  "version": "0.0.1",
  "description": "Git Log Extract",
  "author": "panhezeng <panhezeng@gmail.com>",
  "private": true,
  "main": "packages/main/dist/index.cjs",
  "scripts": {
    "dev": "yarn run lint && node scripts/watch.mjs",
    "type-check": "vue-tsc --noEmit",
    "lint:es": "eslint --fix \"packages/**/src/**/*.{js,jsx,cjs,mjs,ts,tsx,cts,mts,vue}\"",
    "lint:style": "stylelint --fix \"packages/**/src*/**/*.{html,css,js,jsx,ts,tsx,vue,less}\"",
    "lint": "yarn run type-check && cross-env NODE_ENV=production yarn run lint:es && yarn run lint:style",
    "build": "npm run build:main && npm run build:preload && npm run build:renderer",
    "build:main": "cd ./packages/main && vite build",
    "build:preload": "cd ./packages/preload && vite build",
    "build:renderer": "cd ./packages/renderer && vite build",
    "compile": "cross-env MODE=production npm run build && electron-builder build --config .electron-builder.config.js",
    "postinstall": "cross-env ELECTRON_RUN_AS_NODE=1 electron scripts/update-electron-vendors.mjs"
  },
  "devDependencies": {
    "@quasar/vite-plugin": "^1.3.0",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^18.14.0",
    "cross-env": "^7.0.3",
    "electron": "^23.1.0",
    "electron-builder": "^23.6.0",
    "nano-staged": "^0.8.0",
    "simple-git-hooks": "^2.8.1",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-monaco-editor": "^1.1.0",
    "@rushstack/eslint-patch": "^1.2.0",
    "@stylelint/postcss-css-in-js": "^0.38.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.34.0",
    "eslint-plugin-vue": "^9.9.0",
    "less": "^4.1.3",
    "postcss": "^8.4.21",
    "postcss-html": "^1.5.0",
    "postcss-less": "^6.0.0",
    "postcss-syntax": "^0.36.2",
    "prettier": "^2.8.4",
    "prettier-plugin-organize-imports": "^3.2.2",
    "prettier-plugin-packagejson": "^2.4.3",
    "stylelint": "^15.2.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^30.0.1",
    "stylelint-order": "^6.0.2",
    "stylelint-prettier": "^2.0.0",
    "typescript": "~4.9.5",
    "vite": "^4.1.3",
    "vue-tsc": "^1.1.5"
  },
  "dependencies": {
    "@quasar/extras": "^1.15.11",
    "ace-builds": "^1.15.2",
    "jscrypto": "^1.0.3",
    "dayjs": "^1.11.7",
    "electron-store": "^8.1.0",
    "electron-updater": "5.3.0",
    "fs-extra": "^11.1.0",
    "monaco-editor": "^0.35.0",
    "pinia": "^2.0.31",
    "quasar": "^2.11.6",
    "simple-git": "^3.16.1",
    "vue": "3.2.47",
    "vue-router": "^4.1.6"
  }
}
